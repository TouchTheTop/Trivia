/**app.wxss**/
page{
  height: 100%;
}
.container {
  padding: 0px 15px;
  padding-top: 15px;
  box-sizing: content-box;
  background: #f8f8f8;
  overflow: auto;
  height: 100%;
} 

.enter{
  padding: 0px 30px;
}

.blcok{
  font-size: 14px;
  padding: 10px;
  background: #eee;
  margin-top:15px;
  color: #555;
  border-radius: 4px;
}

.blcok image{
  width: 22px;
  height: 22px;
  vertical-align: middle;
  margin-top:-2px;
}



view.tip{
  font-size: 12px;
  color: #666;
}

.tip_box{
  font-size: 14px;
  margin: 30px 0px;
  color:#888;
}

.tip_box text{
  color: #10aeff;
}

.tip_box p{
display: block;
line-height: 24px;
margin-top:8px;
}

p{
  display: block;
  word-break: break-all;

}